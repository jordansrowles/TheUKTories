@model CovidGovResponse 

<govuk-summary-list>
    <govuk-summary-list-row>
        <govuk-summary-list-row-key>
            @Html.DisplayNameFor(model => model.Title)
        </govuk-summary-list-row-key>
        <govuk-summary-list-row-value>
            @Html.DisplayFor(model => model.Title)
        </govuk-summary-list-row-value>
    </govuk-summary-list-row>
    <govuk-summary-list-row>
        <govuk-summary-list-row-key>
            @Html.DisplayNameFor(model => model.Date)
        </govuk-summary-list-row-key>
        <govuk-summary-list-row-value>
            @Html.DisplayFor(model => model.Date)
        </govuk-summary-list-row-value>
    </govuk-summary-list-row>
        <govuk-summary-list-row>
        <govuk-summary-list-row-key>
            @Html.DisplayNameFor(model => model.CovidGovResponseSources)
        </govuk-summary-list-row-key>
        <govuk-summary-list-row-value>
            @Html.DisplayFor(model => model.CovidGovResponseSources.Count)
        </govuk-summary-list-row-value>
    </govuk-summary-list-row>

    <govuk-summary-list-row>
        <govuk-summary-list-row-action asp-page="./Details" asp-route-id="@Model.CovidGovResponseId">Details</govuk-summary-list-row-action>
        <govuk-summary-list-row-action asp-page="./Edit" asp-route-id="@Model.CovidGovResponseId">Edit</govuk-summary-list-row-action>
        <govuk-summary-list-row-action asp-page="./Delete" asp-route-id="@Model.CovidGovResponseId">Delete</govuk-summary-list-row-action>
    </govuk-summary-list-row>
</govuk-summary-list>