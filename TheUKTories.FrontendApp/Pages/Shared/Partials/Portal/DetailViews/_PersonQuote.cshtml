@model PersonQuote

<govuk-summary-list>
    <govuk-summary-list-row>
        <govuk-summary-list-row-key>
            @Html.DisplayNameFor(model => model.Quote)
        </govuk-summary-list-row-key>
        <govuk-summary-list-row-value>
            @Html.DisplayFor(model => model.Quote)
        </govuk-summary-list-row-value>
    </govuk-summary-list-row>

    <govuk-summary-list-row>
        <govuk-summary-list-row-key>
            @Html.DisplayNameFor(model => model.PersonId)
        </govuk-summary-list-row-key>
        <govuk-summary-list-row-value>
            @Html.DisplayFor(model => model.PersonId)
        </govuk-summary-list-row-value>
    </govuk-summary-list-row>

    <govuk-summary-list-row>
        <govuk-summary-list-row-action asp-page="./Details" asp-route-id="@Model.PersonQuoteId">Details</govuk-summary-list-row-action>
        <govuk-summary-list-row-action asp-page="./Edit" asp-route-id="@Model.PersonQuoteId">Edit</govuk-summary-list-row-action>
        <govuk-summary-list-row-action asp-page="./Delete" asp-route-id="@Model.PersonQuoteId">Delete</govuk-summary-list-row-action>
    </govuk-summary-list-row>
</govuk-summary-list>